{
  "manifest_version": 2,
  "name": "SCPper",
  "version": "0.2.1.5",
  "description": "__MSG_MANIFEST_DESCRIPTION__",
  "icons": {
	"128": "SCP-logo-128.png",
	"48": "SCP-logo-48.png",
	"16": "SCP-logo-16.png"
  },
  "author": "Alexander \"FiftyNine\" Krivopalov",
  "default_locale": "en",
  "background": {
	"scripts": ["constants.js", "eventPage.js"],
	"persistent": false
  },
  "options_page": "options.html",
  "content_scripts": [{
        "matches": [
			"http://*/*",
			"https://*/*",		
			"*://www.scp-wiki.net/*", "*://scp-wiki.wikidot.com/*",
			"*://scpfoundation.ru/*", "*://scp-ru.wikidot.com/*",
			"*://ko.scp-wiki.net/*", "*://scp-kr.wikidot.com/*",
			"*://scp-wiki-cn.wikidot.com/*",
			"*://fondationscp.wikidot.com/*",
			"*://scp-wiki.net.pl/*", "*://scp-pl.wikidot.com/*",
			"*://lafundacionscp.wikidot.com/*",
			"*://scp-th.wikidot.com/*",
			"*://ja.scp-wiki.net/*", "*://scp-jp.wikidot.com/*",
                        "*://scp-wiki-de.wikidot.com/*",
                        "*://fondazionescp.wikidot.com/*",
                        "*://scp-ukrainian.wikidot.com/*",
                        "*://scp-pt-br.wikidot.com/*"
		],
        "js": ["constants.js",
		    "scpUtils.js", "linkEnhancer.js",
			"wikiUtils.js", "wikiPage.js", 
			"wikiForum.js", "wikiMain.js",
			"scpMain.js"
		],
		"run_at": "document_start"
  }],    
  "externally_connectable": {
        "matches": [
			"*://www.scp-wiki.net/*", "*://scp-wiki.wikidot.com/*",
			"*://scpfoundation.ru/*", "*://scp-ru.wikidot.com/*",
			"*://ko.scp-wiki.net/*", "*://scp-kr.wikidot.com/*",
			"*://scp-wiki-cn.wikidot.com/*",
			"*://fondationscp.wikidot.com/*",
			"*://scp-wiki.net.pl/*", "*://scp-pl.wikidot.com/*",
			"*://lafundacionscp.wikidot.com/*",
			"*://scp-th.wikidot.com/*",
			"*://ja.scp-wiki.net/*", "*://scp-jp.wikidot.com/*",
                        "*://scp-wiki-de.wikidot.com/*",
                        "*://fondazionescp.wikidot.com/*",
                        "*://scp-ukrainian.wikidot.com/*",
                        "*://scp-pt-br.wikidot.com/*"
		]
  },
  "web_accessible_resources": [
	"scpEventOverrides.js"
  ],
  "permissions": [
    "storage",
	"*://www.scp-wiki.net/*", "*://scp-wiki.wikidot.com/*",
	"*://scpfoundation.ru/*", "*://scp-ru.wikidot.com/*",
	"*://ko.scp-wiki.net/*", "*://scp-kr.wikidot.com/*",
	"*://scp-wiki-cn.wikidot.com/*",
	"*://fondationscp.wikidot.com/*",
	"*://scp-wiki.net.pl/*", "*://scp-pl.wikidot.com/*",
	"*://lafundacionscp.wikidot.com/*",
	"*://scp-th.wikidot.com/*",
	"*://ja.scp-wiki.net/*", "*://scp-jp.wikidot.com/*",
        "*://scp-wiki-de.wikidot.com/*",
        "*://fondazionescp.wikidot.com/*",
        "*://scp-ukrainian.wikidot.com/*",
        "*://scp-pt-br.wikidot.com/*"
  ],
  "key": ""
}